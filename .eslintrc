{
    "rules": {
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "next"
            }
        ],
        "no-use-before-define": [
            "error",
            {
                "variables": false
            }
        ],
        "no-multi-str": 0,
        "indent": [
            "error",
            4
        ],
        "comma-dangle": [
            "off",
            "ignore"
        ],
        "max-len": [
            "error",
            {
                "code": 140
            }
        ],
        "quotes": [
            "error",
            "double"
        ]
    },
    "env": {
        "node": true,
        "mocha": true
    },
    "parserOptions": {
        "ecmaVersion": 8
    }
}
